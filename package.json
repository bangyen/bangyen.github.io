{
  "name": "website",
  "homepage": "https://bangyen.github.io",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.24.7",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@types/pako": "^2.0.4",
    "depcheck": "^1.4.3",
    "gh-pages": "^6.3.0",
    "pako": "^2.1.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.12.0",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.2.1"
  },
  "scripts": {
    "start": "vite",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b gh-pages -d build",
    "build": "vite build",
    "build:analyze": "vite build && du -sh build/ && ls -lh build/assets/*.js | awk '{print $5, $9}'",
    "test": "jest --watchAll=false",
    "test:watch": "jest --watch",
    "test:new": "jest --config jest.config.js --maxWorkers=50%",
    "test:ci": "jest --config jest.config.js --ci --maxWorkers=2",
    "test:coverage": "jest --config jest.config.js --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "stylelint": "stylelint \"src/**/*.{css,scss}\" --config config/stylelint.config.js --allow-empty-input",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix --config config/stylelint.config.js --allow-empty-input",
    "lint:all": "npm run lint && npm run format:check && npm run stylelint",
    "scrape:quizzes": "tsx scripts/scrapeQuizzes.ts",
    "data:update": "tsx scripts/updateResearchData.ts",
    "prepare": "husky install"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --fix --config config/stylelint.config.js --allow-empty-input",
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "@babel/core": "^7.22.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.39.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-jest": "^29.7.0",
    "cheerio": "^1.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^39.0.0",
    "terser": "^5.46.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.4"
  }
}
